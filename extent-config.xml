<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
  <configuration>
    <!-- report theme --> <!-- standard, dark -->
    <theme>standard</theme>
  
    <!-- document encoding -->  <!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>
    
    <!-- protocol for script and stylesheets -->   <!-- defaults to https -->
    <protocol>https</protocol>
    
    <!-- title of the document -->
    <documentTitle>Vision - Execution Report</documentTitle>
    
    <!-- report name - displayed at top-nav -->
    <reportName><![CDATA[
                <img src='http://192.168.1.103:91/Images/logoTVision_r.png' height="50" width="150" />
            ]]></reportName>
    <!-- report headline - displayed at top-nav, after reportHeadline -->
	<reportHeadline>AUTOMATION Report</reportHeadline>
    <!-- global date format override -->  <!-- defaults to yyyy-MM-dd -->
    <dateFormat>yyyy-MM-dd</dateFormat>
    
    <!-- global time format override -->   <!-- defaults to HH:mm:ss -->
    <timeFormat>HH:mm:ss</timeFormat>
    
    <!-- custom javascript -->
    <scripts>
	<![CDATA[
	 $(document).ready(function() {
	 $(".logo-content").hide();
	 $(".logo-container").html("Execution Report");
	 $("#image-zoom").src("src",src);
	 $("#image-zoom").append("src",src);
    });
	]]>
     <!-- <![CDATA[
        $(document).ready(function() {
        
        });
      ]]> -->
    </scripts>
    
    <!-- custom styles -->
    <styles>
      <![CDATA[
        .report-name { padding-left: 10px; } .report-name > img { float: left;height: 90%;margin-left: 30px;margin-top: 2px;width: auto; }
      ]]>
    </styles>
  </configuration>
</extentreports>